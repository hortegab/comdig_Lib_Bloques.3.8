      \  \n#    \"\"\"convierte de unipolar a bipolar una senal tipo char\"\"\"\n\
      \    def __init__(self,):  # only default arguments here\n        gr.sync_block.__init__(\n\
      \            self,\n            name='e_unipolar to bipolar_bb',   # will show\
